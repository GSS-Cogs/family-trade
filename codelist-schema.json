{
  "fields": [
    {
      "name": "Label",
      "constraints": {
        "required": true
      }
    }, {
      "name": "Notation",
      "constraints": {
        "required": true,
        "unique": true,
        "pattern": "^-?[\\w\\/]+(-[\\w\\/]+)*$"
      }
    }, {
      "name": "Parent Notation",
      "constraints": {
        "required": false,
        "pattern": "^(-?[\\w\\/]+(-[\\w\\/]+)*|)$"
      }
    }, {
      "name": "Sort Priority",
      "constraints": {
        "required": false
      }
    }, {
      "name": "Description",
      "constraints": {
        "required": false
      }
    }
  ]
}
