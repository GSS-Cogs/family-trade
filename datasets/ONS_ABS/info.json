{
    "id": "ons_abs",
    "title": "Annual Business Survey exporters and importers",
    "publisher": "Office for National Statistics",
    "description": "Exporters and importers of goods and services in Great Britain by employment size, turnover size, ownership and age.",
    "landingPage": "https://www.ons.gov.uk/businessindustryandtrade/business/businessservices/datasets/annualbusinesssurveyimportersandexporters",
    "datasetNotes": [
        "On landing page, latest version"
    ],
    "published": "2019-11-25",
    "families": [
        "Trade"
    ],
    "extract": {
        "source": "XLS",
        "stage": "Done"
    },
    "transform": {
        "airtable": "rec6SF5JMLyIYm8z9",
        "main_issue": 32,
        "columns": {
            "Year": {
                "type": "dimension",
                "uri": "http://purl.org/linked-data/sdmx/2009/dimension#refPeriod",
                "value": "http://reference.data.gov.uk/id/{+year}"
            },
            "Age of Business": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/trade/property/dimension/age-of-business",
                "value": "http://gss-data.org.uk/def/trade/concept/age-of-business/{+age_of_business}"
            },
            "Business Activity": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/trade/property/dimension/business-activity",
                "value": "http://gss-data.org.uk/def/trade/concept/business-activity/{+business_activity}"
            },
            "Country of Ownership": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/trade/property/dimension/country-of-ownership",
                "value": "http://gss-data.org.uk/def/trade/concept/country-of-ownership/{+country_of_ownership}"
            },
            "Trade Group": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/trade/property/dimension/trade-group",
                "value": "http://gss-data.org.uk/def/trade/concept/trade-group/{+trade_group}"
            },
            "Turnover": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/trade/property/dimension/turnover-size-bands",
                "value": "http://gss-data.org.uk/def/trade/concept/turnover-size-bands/{+turnover}"
            },
            "Employees": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/trade/property/dimension/employment-size-band",
                "value": "http://gss-data.org.uk/def/trade/concept/employment-size-bands/{+employees}"
            },
            "Value": {
                "type": "observations",
                "measure": "http://gss-data.org.uk/def/measure/percentage",
                "unit": "http://gss-data.org.uk/def/concept/measurement-units/businesses",
                "datatype": "integer"
            },
            "Marker": {
                "type": "attribute",
                "uri": "http://purl.org/linked-data/sdmx/2009/attribute#obsStatus",
                "value": "http://gss-data.org.uk/def/concept/cogs-markers/{+marker}"
            }
        }
    },
    "load": {
        "publish": true
    },
