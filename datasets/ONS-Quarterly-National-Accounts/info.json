{
    "$schema": "http://gss-cogs.github.io/family-schemas/dataset-schema-1.1.0.json",
    "id": "ons-quarterly-national-accounts", 
    "title": "Quarterly National Accounts", 
    "publisher": "Office for National Statistics", 
    "description": "Annual and quarterly data for UK gross domestic product (GDP) estimates, in chained volume measures and current market prices.", 
    "landingPage": "https://www.ons.gov.uk/economy/grossdomesticproductgdp/datasets/uksecondestimateofgdpdatatables", 
    "datasetNotes": ["On landing page, latest version of First Estimate, separately latest version of quarterly national accounts"], 
    "published": "2020-02-11", 
    "families": ["Trade"], 
    "extract": {
        "source": "XLS", 
        "stage": "Prioritized"
    }, 
    "transform": {
        "airtable": "recvRRELh7LMjqFtF", 
        "columns": {
            "Period": {
                "type": "dimension",
                "uri": "http://purl.org/linked-data/sdmx/2009/dimension#refPeriod", 
                "value": "http://reference.data.gov.uk/id/{+period}"
            }, 
            "CDID": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/trade/property/dimension/cdid", 
                "value": "http://gss-data.org.uk/def/trade/concept/cdid/{+cdid}"
            }, 
            "Estimate Type": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/trade/property/dimension/national-accounts-estimate-type", 
                "value": "http://gss-data.org.uk/def/trade/concept/national-accounts-estimate-type/{+estimate_type}"
            }, 
            "Aggregate": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/trade/property/dimension/national-accounts-aggregate", 
                "value": "http://gss-data.org.uk/def/trade/concept/national-accounts-aggregate/{+aggregate}"
            }, 
            "Industry": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/trade/property/dimension/national-accounts-industry", 
                "value": "http://gss-data.org.uk/def/trade/concept/national-accounts-industry/{+industry}"
            }, 
            "Sector": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/trade/property/dimension/national-accounts-industry", 
                "value": "http://gss-data.org.uk/def/trade/concept/national-accounts-industry/{+sector}"
            }, 
            "Expenditure Category": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/trade/property/dimension/national-accounts-expenditure", 
                "value": "http://gss-data.org.uk/def/trade/concept/national-accounts-expenditure/{+expenditure_category}"
            }, 
            "Economic Concept": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/trade/property/dimension/national-accounts-estimate-type", 
                "value": "http://gss-data.org.uk/def/trade/concept/national-accounts-estimate-type/{+economic_concept}"
            }, 
            "Category of Income": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/trade/property/dimension/national-accounts-income-indicator", 
                "value": "http://gss-data.org.uk/def/trade/concept/national-accounts-income-indicator/{+category_of_income}"
            }, 
            "COICOP": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/property/dimension/coicop", 
                "value": "http://gss-data.org.uk/def/concept/coicop/{+coicop}"
            }, 
            "Capital Formation": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/trade/property/dimension/national-accounts-capital-formation", 
                "value": "http://gss-data.org.uk/def/trade/concept/national-accounts-capital-formation/{+capital_formation}"
            }, 
            "Analysis": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/trade/property/dimension/national-accounts-analysis", 
                "value": "http://gss-data.org.uk/def/trade/concept/national-accounts-analysis/{+analysis}"
            }, 
            "Goods or Services": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/trade/property/dimension/national-accounts-trade-type", 
                "value": "http://gss-data.org.uk/def/trade/concept/national-accounts-trade-type/{+goods_or_services}"
            }, 
            "Flow": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/trade/property/dimension/flow-directions", 
                "value": "http://gss-data.org.uk/def/trade/concept/flow-directions/{+flow}"
            }, 
            "2019 Weights": {
                "type": "attribute"
            }, 
            "Level of inventories held at end-December 2019": {
                "type": "attribute",
                "uri": "http://gss-data.org.uk/def/trade/property/attribute/level-of-inventories-held-at-end-december-2019",
                "value":"http://gss-data.org.uk/def/trade/concept-scheme/Level-of-inventories-held-at-end-December-2019/{+Level_of _inventories_held_at_end_December_2019}"
            },
            "Value": {
                "type": "observations",
                "unit": "http://gss-data.org.uk/def/concept/measurement-units/gbp-million", 
                "measure": "http://gss-data.org.uk/def/measure/gdp", 
                "datatype": "integer"
            }, 
            "Marker": {
                "type": "attribute",
                "uri": "http://purl.org/linked-data/sdmx/2009/attribute#obsStatus", 
                "value": "http://gss-data.org.uk/def/concept/cogs-markers/{+marker}"
            }
            }, 
            "main_issue": 13
        }, 
        "sizingNotes": "", 
        "notes": ""
    }