            },
            "Country Transaction": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/trade/property/dimension/country-transaction",
                "value": "http://gss-data.org.uk/def/trade/concept/country-transaction/{+country_transaction}"
