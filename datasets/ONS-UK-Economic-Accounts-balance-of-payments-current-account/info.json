            "Income Description": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/trade/property/dimension/income-description",
                "value": "http://gss-data.org.uk/def/trade/concept/income-description/{+income_description}"
            },
            "Earnings": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/trade/property/dimension/earnings",
                "value": "http://gss-data.org.uk/def/trade/concept/earnings/{+earnings}"
            },
            "Product": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/trade/property/dimension/product",
                "value": "http://gss-data.org.uk/def/trade/concept/product/{+product}"
            },
            "Transaction Type": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/trade/property/dimension/transaction-type",
