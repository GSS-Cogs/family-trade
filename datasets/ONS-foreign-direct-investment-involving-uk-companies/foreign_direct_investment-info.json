{
  "$schema": "http://gss-cogs.github.io/family-schemas/dataset-schema-1.1.0.json",
  "id": "ons-foreign-direct-investment-involving-uk-companies",
  "title": "Foreign direct investment involving UK companies: inward",
  "publisher": "Office for National Statistics",
  "description": "Annual statistics on the investment of foreign companies into the UK, including for investment flows, positions and earnings.",
  "landingPage": "https://www.ons.gov.uk/businessindustryandtrade/business/businessinnovation/datasets/foreigndirectinvestmentinvolvingukcompanies2013inwardtables",
  "datasetNotes": [
    "On landing page, latest version"
  ],
  "published": "2020-02-06",
  "families": [
    "Trade"
  ],
  "extract": {
    "source": "XLS",
    "stage": "Done"
  },
  "transform": {
    "airtable": [
      "reclbpcTgt5imCd6y",
      "recoIg2XstRRBKPfL"
    ],
    "main_issue": 28,
    "columns": {
      "Investment Direction": {
        "type": "dimension",
        "uri": "http://gss-data.org.uk/def/trade/property/dimension/flow-directions",
        "value": "http://gss-data.org.uk/def/trade/concept/flow-directions/{+investment_direction}"
      },
      "Year": {
        "type": "dimension",
        "new": {
          "subPropertyOf": "http://purl.org/linked-data/sdmx/2009/dimension#refPeriod",
          "codelist": true
        },
        "value": "http://reference.data.gov.uk/id/year/{+year}"
      },
      "FDI Component":{
        "type":"dimension",
        "uri":"http://gss-data.org.uk/def/trade/property/dimension/FDI-component",
        "value":"http://gss-data.org.uk/def/trade/concept/fdi-component/{+FDI_component}"
      },
      "FDI Area": {
        "type": "dimension",
        "uri": "http://gss-data.org.uk/def/trade/property/dimension/FDI-area",
        "value": "http://gss-data.org.uk/def/trade/concept/fdi-area/{+FDI_area}"
      },
      "FDI Industry": {
        "type": "dimension",
        "uri": "http://gss-data.org.uk/def/trade/property/dimension/FDI-industry",
        "value": "http://gss-data.org.uk/def/trade/concept/fdi-industry/{+FDI_industry}"
      },
      "International Trade Basis": {
        "type": "dimension",
        "uri": "http://gss-data.org.uk/def/trade/property/dimension/international-trade-basis",
        "value": "http://gss-data.org.uk/def/trade/concept/international-trade-basis/{international_trade_basis}"
      },
      "Value": {
        "type": "observations",
        "measure": {
          "label": "gbp-total"
        },
        "unit": {
          "label": "gbp-million"
        }
      },
      "Marker": {
        "type": "attribute",
        "uri": "http://purl.org/linked-data/sdmx/2009/attribute#obsStatus",
        "value": "http://gss-data.org.uk/def/concept/cogs-markers/{+marker}"
      }
    }
  },
  "sizingNotes": "",
  "notes": "Published in PMD combined with FDI: outward"
}