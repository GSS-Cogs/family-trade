    "datasetNotes": [
        "On landing page, latest version"
    ],
    "published": "2020-02-06",
    "families": [
        "Trade"
    ],
    "extract": {
        "source": "XLS",
        "stage": "Done"
    },
    "transform": {
        "airtable": [
            "reclbpcTgt5imCd6y",
            "recoIg2XstRRBKPfL"
        ],
        "main_issue": 28,
        "columns": {
            "Investment Direction": {
                "type": "dimension",
                "uri": "http://gss-data.org.uk/def/trade/property/dimension/flow-directions",
                "value": "http://gss-data.org.uk/def/trade/concept/flow-directions/{investment_direction}"
            },
            "Year": {
                "type": "dimension",
                "new": {
                    "subPropertyOf": "http://purl.org/linked-data/sdmx/2009/dimension#refPeriod",
                    "codelist": false
                },
                "value": "http://reference.data.gov.uk/id/year/{year}"
            },
            "FDI Area": {
                "type": "dimension",
                "new": {
                    "subPropertyOf": "http://purl.org/linked-data/sdmx/2009/dimension#refArea",
                    "codelist": true
                }
            },
            "FDI Industry": {
                "type": "dimension",
                "new": {
                    "subPropertyOf": "http://gss-data.org.uk/def/trade/property/dimension/industry",
