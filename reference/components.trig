<http://gss-data.org.uk/graph/trade-components> {
	<http://gss-data.org.uk/def/ontology/trade-components> dcterms:title "Trade Components Ontology"@en ;
		rdfs:label "Components Ontology"@en ;
		a owl:Ontology .

	<http://gss-data.org.uk/def/dimension/gdp-estimate-type> rdfs:label "Estimate Type" ;
		a qb:DimensionProperty ;
		qb:codeList <http://gss-data.org.uk/def/concept-scheme/gdp-estimate-type> ;
		skos:notation "gdp-estimate-type" ;
		rdfs:range <http://gss-data.org.uk/def/GdpEstimateType> ;
		rdfs:isDefinedBy <http://gss-data.org.uk/def/ontology/trade-components> ;
		a rdf:Property .
	
	<http://gss-data.org.uk/def/dimension/gdp-reference-period> rdfs:label "Reference Period" ;
		dcterms:description "Available reference point" ;
		a qb:DimensionProperty ;
		skos:notation "gdp-reference-period" ;
		rdfs:range <http://gss-data.org.uk/def/GdpReferencePeriod> ;
		rdfs:isDefinedBy <http://gss-data.org.uk/def/ontology/trade-components> ;
		rdfs:subPropertyOf <http://purl.org/linked-data/sdmx/2009/dimension#refPeriod> ;
		a rdf:Property .
